{
    "version": "0.1.0",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "package": "tsdx build && yarn link",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint src",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook -o docs-build"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "singleQuote": true,
        "tabWidth": 4,
        "semi": true,
        "useTabs": false,
        "trailingComma": "all",
        "arrowParens": "always",
        "proseWrap": "always",
        "endOfLine": "lf"
    },
    "name": "frontend-components",
    "author": "Hafiz Temuri",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "module": "dist/frontend-components.esm.js",
    "size-limit": [
        {
            "path": "dist/frontend-components.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/frontend-components.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.16.7",
        "@mui/types": "^7.1.0",
        "@size-limit/preset-small-lib": "^7.0.5",
        "@storybook/addon-essentials": "^6.4.13",
        "@storybook/addon-info": "^5.3.21",
        "@storybook/addon-links": "^6.4.13",
        "@storybook/addons": "^6.4.13",
        "@storybook/react": "^6.4.13",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "babel-loader": "^8.2.3",
        "husky": "^7.0.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-is": "^17.0.2",
        "size-limit": "^7.0.5",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@mui/material": "^5.3.0",
        "@mui/styles": "^5.2.3"
    },
    "resolutions": {
        "**/@emotion/styled": "^11.0.0"
    }
}